{
  "name": "auth-service",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://gitlab.com/ivan-studies/rewrite/atomic/auth-service.git",
  "author": "Ivan Guimar√£es Monte <ivg.monte@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "build:prod": "npm run type-check && npm run build",
    "db:migration:run": "./node_modules/.bin/ts-node -r ./tsconfig-paths-bootstrap.js ./node_modules/.bin/typeorm migration:run",
    "db:start": "sh start_db.sh",
    "db:stop": "docker stop $(grep DB_CONTAINER .env | cut -d '=' -f2)",
    "lint": "./node_modules/.bin/eslint src/**/*.ts --fix",
    "prettier": "./node_modules/.bin/prettier --config ./.prettierrc.js",
    "prettier:write": "npm run prettier -- --write",
    "prettier:check": "npm run prettier -- --check",
    "start": "ts-node src/index.ts",
    "start:debug": "npm run build && NODE_ENV=development node -r dotenv/config -r ./tsconfig-paths-bootstrap.js --nolazy --inspect-brk=9229 dist/src/index.js",
    "start:dev": "NODE_ENV=development node -r dotenv/config -r ./tsconfig-paths-bootstrap.js dist/src/index.js",
    "start:watch": "nodemon",
    "test": "NODE_ENV=test jest --no-cache --runInBand --detectOpenHandles --testTimeout=15000",
    "type-check": "./node_modules/.bin/tsc -p . --noEmit"
  },
  "dependencies": {
    "@hapi/joi": "16.1.8",
    "@overnightjs/core": "1.6.12",
    "axios": "0.21.1",
    "express": "4.17.1",
    "express-async-errors": "3.1.1",
    "express-fileupload": "1.1.9",
    "express-graphql": "0.9.0",
    "fuse.js": "5.2.2",
    "graphql": "14.1.1",
    "graphql-yoga": "1.18.3",
    "http-status-codes": "1.4.0",
    "inversify": "5.0.1",
    "lodash": "4.17.19",
    "moment": "2.25.3",
    "music-metadata": "^6.3.7",
    "mysql2": "2.0.1",
    "reflect-metadata": "0.1.10",
    "socket.io": "2.3.0",
    "tsconfig-paths": "3.9.0",
    "type-graphql": "0.17.6",
    "typeorm": "0.2.24",
    "typeorm-factories": "1.0.1"
  },
  "devDependencies": {
    "@nestjs/common": "7.0.8",
    "@types/bluebird": "3.5.29",
    "@types/express": "4.17.2",
    "@types/express-fileupload": "1.1.3",
    "@types/faker": "4.1.11",
    "@types/graphql": "14.5.0",
    "@types/hapi__joi": "16.0.6",
    "@types/jest": "24.0.23",
    "@types/lodash": "4.14.150",
    "@types/node": "12.12.14",
    "@types/socket.io": "2.1.4",
    "@types/supertest": "2.0.8",
    "@types/validator": "12.0.1",
    "@typescript-eslint/eslint-plugin": "2.10.0",
    "@typescript-eslint/parser": "2.10.0",
    "axios-mock-adapter": "1.17.0",
    "dotenv": "8.2.0",
    "dotenv-cli": "3.1.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "faker": "4.1.0",
    "jest": "24.9.0",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "supertest": "4.0.2",
    "ts-jest": "24.2.0",
    "ts-node": "8.9.0",
    "typescript": "3.8.3",
    "wait-for-mysql": "1.0.0"
  }
}